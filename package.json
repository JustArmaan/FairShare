{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon -e ts,tsx,html -w src/server -x bun run src/server/main.ts",
    "start": "NODE_ENV=production bun src/server/main.ts",
    "build": "vite build",
    "tailwind-watch": "bun x tailwindcss -i ./src/server/views/tailwind.css -o ./public/output.css --watch",
    "drizzle-generate": "drizzle-kit generate:sqlite",
    "drizzle-seed": "bun run src/database/seed.ts",
    "drizzle-push": "drizzle-kit push:sqlite"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@kinde-oss/kinde-node-express": "^1.3.0",
    "@kinde-oss/kinde-typescript-sdk": "^2.9.0",
    "@libsql/client": "^0.5.3",
    "@types/connect-livereload": "^0.6.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/livereload": "^0.9.5",
    "@vitejs/plugin-react": "^4.2.1",
    "better-sqlite3": "^9.4.3",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.30.6",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "jsxte": "^3.3.1",
    "nodemon": "^3.1.0",
    "open": "^10.1.0",
    "react": "^18.3.1",
    "tsx": "^4.7.3",
    "vite": "^5.2.10",
    "vite-express": "^0.16.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "tailwindcss": "^3.4.1"
  },
  "module": "src/index.js"
}
