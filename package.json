{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon -e ts,tsx,html -w src -x bun run src/server/main.ts",
    "start": "bun src/server/main.ts",
    "build": "vite build",
    "tailwind-watch": "bun x tailwindcss -i ./src/server/views/tailwind.css -o ./public/output.css --watch",
    "tailwind-build": "bun x tailwindcss -i ./src/server/views/tailwind.css -o ./public/output.css",
    "drizzle-generate": "drizzle-kit generate:sqlite",
    "drizzle-seed": "bun run src/server/database/seed.ts",
    "drizzle-push": "drizzle-kit push:sqlite",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@kinde-oss/kinde-node-express": "^1.3.0",
    "@kinde-oss/kinde-typescript-sdk": "^2.9.0",
    "@libsql/client": "^0.5.3",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.17",
    "drizzle-orm": "^0.30.6",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "jsxte": "^3.3.1",
    "nodemon": "^3.1.0",
    "open": "^10.1.0",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "tailwind-hamburgers": "^1.3.5",
    "tsx": "^4.7.3",
    "uuid": "^9.0.1",
    "vite": "^5.2.10",
    "vite-express": "^0.16.0"
  },
  "devDependencies": {
    "@types/connect-livereload": "^0.6.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/livereload": "^0.9.5",
    "@types/mime": "^4.0.0",
    "better-sqlite3": "^9.6.0",
    "bun-types": "latest",
    "tailwindcss": "^3.4.1"
  },
  "module": "src/index.js"
}
