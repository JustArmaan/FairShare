{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon -e ts,tsx,html -w src -x bun run src/server/main.tsx",
    "start": "bun src/server/main.tsx",
    "build": "vite build",
    "tailwind-watch": "bun x tailwindcss -i ./src/server/views/tailwind.css -o ./public/output.css --watch",
    "tailwind-build": "bun x tailwindcss -i ./src/server/views/tailwind.css -o ./public/output.css",
    "generate": "drizzle-kit generate",
    "seed": "bun run src/server/database/seed.ts",
    "migrate": "drizzle-kit migrate",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio",
    "turso-local": "turso dev",
    "db-setup": "bun run generate && bun run push && bun run seed && echo 'Database Initialized!'",
    "typecheck": "tsc --noEmit"
  },
  "typings": "/index.d.ts",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@kinde-oss/kinde-typescript-sdk": "^2.9.0",
    "@libsql/client": "^0.5.3",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/google.maps": "^3.55.8",
    "@types/react": "^18.3.1",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.21.4",
    "drizzle-orm": "^0.30.6",
    "express": ">=5.0.0-beta.1",
    "htmx.org": "^1.9.12",
    "jsxte": "^3.3.1",
    "nodemon": "^3.1.0",
    "open": "^10.1.0",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-hamburgers": "^1.3.5",
    "tsx": "^4.7.3",
    "uuid": "^9.0.1",
    "vite": "^5.2.10",
    "vite-express": "^0.16.0"
  },
  "devDependencies": {
    "@types/connect-livereload": "^0.6.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/googlemaps": "^3.43.3",
    "@types/livereload": "^0.9.5",
    "@types/mime": "^4.0.0",
    "bun-types": "latest",
    "tailwindcss": "^3.4.1",
    "vite-plugin-pwa": "^0.20.0"
  },
  "module": "src/index.js"
}
